{
  "name": "falixnodes",
  "version": "4.28.0",
  "description": "",
  "main": "src/js/electron/index.js",
  "scripts": {
    "start": "electron --enable-transparent-visuals --disable-gpu .",
    "build": "build",
    "rebuild": "electron-rebuild"
  },
  "author": "KorbsStudio",
  "license": "MIT",
  "dependencies": {
    "electron-tools": "^2.4.2",
    "glasstron": "^0.0.7",
    "node-abi": "^2.30.0",
    "node-addon-api": "^3.0.0",
    "node-pty": "^0.11.0-beta7",
    "npm": "^7.14.0",
    "os-utils": "^0.0.14",
    "x11": "^2.3.0",
    "xterm": "^4.8.1"
  },
  "devDependencies": {
    "electron": "^13.0.1",
    "electron-builder": "^19.0.0",
    "electron-rebuild": "^1.11.0"
  },
  "build": {
    "protocols": {
      "name": "falix",
      "schemes": [
        "falix"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "snap",
      "maintainer": "package.korbsstudio.falixnodes",
      "category": "Network",
      "vendor": "Korbs Studio",
      "description": "Accessing Falix, all from one place"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "shortcutName": "Falix Software"
    }
  }
}
